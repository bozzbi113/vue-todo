{
  "name": "com.nashtech.liferay.vue.ui",
  "version": "1.0.0",
  "description": "Vue Js Liferay Template",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@mdi/font": "^4.8.95",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "7.1.5",
    "copy-webpack-plugin": "4.6.0",
    "deepmerge": "^4.2.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.2.2",
    "liferay-npm-build-support": "^2.17.0",
    "liferay-npm-bundler": "^2.17.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "2.6.7",
    "vuetify-loader": "^1.3.0",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap-select": "^1.13.12",
    "material-design-icons-iconfont": "^5.0.1",
    "moment": "^2.24.0",
    "vue": "2.6.7",
    "vue-router": "2.7.0",
    "vuejs-paginate": "^2.1.0",
    "vuelidate": "^0.7.4",
    "vuetify": "^2.2.4",
    "vuex": "^3.1.2"
  },
  "scripts": {
    "dev": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "babel --source-maps -d build src && vue-cli-service build --dest build/ --formats commonjs --target lib --name index ./src/index.js && npm run copy-assets && liferay-npm-bundler",
    "copy-assets": "lnbs-copy-assets",
    "deploy": "npm run build && lnbs-deploy",
    "start": "lnbs-start",
    "translate": "lnbs-translate"
  },
  "portlet": {
    "com.liferay.portlet.display-category": "Vue Js Liferay",
    "com.liferay.portlet.header-portlet-css": "/index.css",
    "com.liferay.portlet.instanceable": true,
    "javax.portlet.name": "com_nashtech_vuejs_ui",
    "javax.portlet.display-name": "Vue Js Liferay Template",
    "javax.portlet.security-role-ref": "power-user,user",
    "javax.portlet.resource-bundle": "content.Language"
  },
  "main": "index.common.js"
}
